{
	"name": "@xstyled-theme/cli",
	"version": "0.3.0",
	"description": "A CLI that builds Xstyled themes from assets, and helpers to use them in your project",
	"homepage": "/",
	"license": "MIT",
	"author": "Steve Dodier-Lazaro <steve@lajavaness.com>",
	"main": "src/index.js",
	"bin": {
		"xstyled-theme": "bin/xstyled-theme"
	},
	"scripts": {
		"cli": "node --unhandled-rejections=strict bin/xstyled-theme",
		"cli:dev": "cross-env NODE_ENV=development yarn cli",
		"cli:prod": "cross-env NODE_ENV=production yarn cli",
		"lint": "yarn eslint --ext .js,.jsx src",
		"prepublishOnly": "yarn test:ci && yarn lint",
		"prettier": "scripts/precommitHookPrettier.sh",
		"test": "node scripts/test.js -u",
		"test:ci": "cross-env CI=true yarn test -b --watchAll=false --verbose --bail"
	},
	"dependencies": {
		"@babel/runtime": "^7.9.2",
		"@hapi/joi": "^17.1.1",
		"chalk": "^4.0.0",
		"dotenv": "^8.2.0",
		"fontface-styled-components": "^0.3.0",
		"inquirer": "^7.0.0",
		"js-levenshtein": "^1.1.6",
		"lodash": "^4.17.15",
		"minimist": "^1.2.0",
		"mkdirp": "^1.0.4",
		"rimraf": "^3.0.0",
		"sort-package-json": "^1.42.0",
		"svg-sprite": "^1.5.0",
		"svgo": "^1.3.2",
		"typescript": "^3.9.3"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-export-default-from": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/polyfill": "^7.8.7",
		"@babel/preset-env": "^7.9.5",
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"@commitlint/prompt-cli": "^8.2.0",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/commit-analyzer": "^8.0.1",
		"@semantic-release/git": "^9.0.0",
		"@semantic-release/npm": "^7.0.5",
		"@semantic-release/release-notes-generator": "^9.0.1",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^25.3.0",
		"babel-loader": "^8.1.0",
		"cross-env": "^7.0.2",
		"dotenv-expand": "^5.1.0",
		"env-cmd": "^10.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-config-react-app": "^5.2.1",
		"eslint-loader": "^4.0.0",
		"eslint-plugin-flowtype": "^4.7.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^3.0.0",
		"file-loader": "^6.0.0",
		"husky": "^4.2.5",
		"jest": "^25.3.0",
		"jest-environment-jsdom-sixteen": "^1.0.3",
		"jest-watch-typeahead": "^0.5.0",
		"pnp-webpack-plugin": "^1.6.4",
		"prettier": "^2.0.4",
		"react-dev-utils": "^10.2.1",
		"semantic-release": "^17.0.4",
		"speed-measure-webpack-plugin": "^1.3.3"
	},
